            Var_hu_map_notes
          )
          
        } else {
          create_new_project_query <- create_new_project_approval(
            var_project_id,
            var_project_name,
            var_description,
            var_status,
            var_start_date,
            var_estimated_comp,
            var_act_comp_date,
            var_compl_pct,
            var_proj_size_est,
