    var_approv_req      <- paste0("'",user_requestor,"'")
    var_submitter_id    <- paste0(team_id, ",'")  
        
        if(supervisor_flag  > 0){
          create_new_project_query <- create_new_project(
            var_project_id,
            var_project_name,
            var_description,
            var_status,
            var_start_date,
            var_estimated_comp,
            var_act_comp_date,
            var_compl_pct,
